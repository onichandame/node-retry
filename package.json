{
  "name": "@onichandame/retry",
  "type": "module",
  "version": "1.0.1",
  "description": "modern retry with simplicity",
  "main": "dist/cjs/index",
  "module": "dist/esm/index.js",
  "scripts": {
    "test": "jest",
    "build:cjs": "tsc -p ./tsconfig.build-cjs.json",
    "build:esm": "tsc -p ./tsconfig.build-esm.json",
    "build": "concurrently 'yarn build:cjs' 'yarn build:esm'"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onichandame/node-retry.git"
  },
  "author": "onichandame",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/onichandame/node-retry/issues"
  },
  "homepage": "https://github.com/onichandame/node-retry#readme",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^16.6.1",
    "concurrently": "^6.2.1",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.5"
  }
}
